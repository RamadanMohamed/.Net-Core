@model IEnumerable<Student>
@{
    ViewBag.Title = "Student list";
}

<table class="table">
    <tr>
        <th>
            ID
        </th>
        <th>
            Name
        </th>
        <th>
            Age
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.id
            </td>
            <td>
                @item.name
            </td>
            <td>
                @item.Age
            </td>

            <td></td>
        </tr>
    }

</table>
